using System.Diagnostics;

class Program
{
    static void Main()
    {
        Console.WriteLine("Введите количество цифр: ");
        int kolvo = int.Parse(Console.ReadLine());
        for (int i = 0; i < kolvo; i++)
        {
            Process.Start("notepad.exe");
        }
        Console.WriteLine("Запущено копий: " + kolvo);
    }
}
